generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Vehicle {
  id        String   @id @default(uuid())
  brand     String
  model     String
  year      Int
  color     String
  price     Float
  plate     String   @unique
  status    String   @default("AVAILABLE")
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

